{
  "GmWeb": {
    "BaseDomain": "goodmojo.test",
    "Api": {
      "Name": "Api",
      "PathPrefix": "v1"
    },
    "Identity": {
      "Name": "Identity"
    },
    "RHI": {
      "Name": "Rhi"
    },
    "ConfirmEmailChange": { "Path": "Account/ConfirmEmailChange" },
    "ConfirmEmail": { "Path": "Account/ConfirmEmail" },
    "ResetPassword": { "Path": "Account/ResetPassword" },
    "Login": { "Path": "Account/Login" },
    "Logout": { "Path": "Account/Logout" }
  },
  "DatabaseConnections": {
    "Default": {
      "CommandTimeout": 600
    }
  },
  "Services": {
    "QRCode": {
      "PixelsPerModule": 3,
      "DrawQuietZones": true,
      "ImageFormat": "Png",
      "Compression": "GZip",
      "ECCLevel": "H",
      "DarkColor": "#13586B",
      "LightColor": "#E8E8E8"
    }
  },
  "RedisCache": {
    "Enabled": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.SqlServer": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "Console": {
      "FormatterName": "GM",
      "FormatterOptions": {
        "ColorBehavior": "Enabled",
        "SingleLine": true,
        "IncludeScopes": false,
        "IncludeCategories": false,
        "IncludeEvents": false,
        "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
        "UseUtcTimestamp": false
      }
    }
  }
}
